<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>TRMS_backend</display-name>
  <welcome-file-list>
    <welcome-file>/html/home.html</welcome-file>
  </welcome-file-list>
  
                    <!-- Login -->
  <servlet>
    <servlet-name>loginservlet</servlet-name>
    <servlet-class>com.revature.servlets.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginservlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/html/login.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/loginpage</url-pattern>
  </servlet-mapping>
  
                    <!-- Welcome -->
  <servlet>
    <servlet-name>Welcome</servlet-name>
    <jsp-file>/html/welcome.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Welcome</servlet-name>
    <url-pattern>/welcome</url-pattern>
  </servlet-mapping>
  
                      <!-- Register -->
  <servlet>
    <servlet-name>Register</servlet-name>
    <jsp-file>/html/register.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  <servlet>
     <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>com.revature.servlets.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/registerservlet</url-pattern>
  </servlet-mapping>
  
  
                        <!-- Reimburse -->
   <servlet>
    <servlet-name>ReimbursementForm</servlet-name>
    <jsp-file>/html/reimburse.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReimbursementForm</servlet-name>
    <url-pattern>/reimburse</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ReimbursementFormServlet</servlet-name>
    <servlet-class>com.revature.servlets.ReimbursementFormServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ReimbursementFormServlet</servlet-name>
    <url-pattern>/reimburseservlet</url-pattern>
  </servlet-mapping>
  
                        <!-- Admin -->
  <servlet>
    <servlet-name>Admin</servlet-name>
    <jsp-file>/html/admin.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>Admin</servlet-name>
    <url-pattern>/admin</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>AdminServlet</servlet-name>
    <servlet-class>com.revature.servlets.AdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AdminServlet</servlet-name>
    <url-pattern>/adminservlet</url-pattern>
  </servlet-mapping>
 
                         <!-- FileUpload -->   
    <servlet>
    <servlet-name>FileUp</servlet-name>
    <jsp-file>/html/fileupload.html</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUp</servlet-name>
    <url-pattern>/fileupload</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>FileUpload</servlet-name>
    <servlet-class>com.revature.servlets.FileUpload</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FileUpload</servlet-name>
    <url-pattern>/fileuploadservlet</url-pattern>
  </servlet-mapping>
   
</web-app>